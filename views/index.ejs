<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <link rel="stylesheet" href="css/styles.css">
  <title>Map</title>
</head>
<body>
  <header class="app-header">
    <nav>
      <ul class="app-nav">
        <li>
          <a class="app-nav-ref" href="#">Profile</a>
        </li>
        <li>
          <a class="app-nav-ref" href="#">Locations</a>
        </li>
        <li>
          <a class="app-nav-ref" href="#">Statistics</a>
        </li>
        <li>
          <a class="app-nav-ref" href="#">Settings</a>
        </li>
      </ul>       
        <form action="logout" method="POST">
          <input type="submit" value="logout" class="logout">
        </form>
    </nav>
    </header>
    
    <div id="map"></div>
  
    <div class="loc-highlight" id="loc-highlight"></div>
        <div class="curr-loc-highlight" id="curr-loc-highlight"></div>
        

  <div class="loc-info" id="output">
    <h2 class="info-heading" id="click-loc-heading"></h2>
    <br>
    Location coords: <span id="click-location"></span>
  </div>
  <div class="current-loc-info" id="current-loc-info">
    <h2 class="info-heading">Current location</h2>
    User coords: <span id="users-geocords"></span>
    <br>
    Location coords: <span id="users-location"></span>
    <button class="occupy-btn" id="occupy-btn">Occupy</button>
    </div>
    
    <!-- <div id="info-box"></div> -->
  <script src="js/map2.js"></script>

        <script async defer
        src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDVvJYiAmFhsqumbf3mNFYaBvHJczJSrh0&callback=initMap">
        </script>
    
</body>
</html>